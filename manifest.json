{
  "manifest_version": 2,
  "name": "Gogoout 開發工具",
  "description": "Gogoout 開發工具",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://gogooutlaravel.data/*",
        "https://ccore.spgateway.com/*"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}